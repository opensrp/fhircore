{
  "appId": "app",
  "configType": "navigation",
  "menuActionButton": {
    "id": "mainActionButton",
    "visible": true,
    "display": "{{ add.new.household }}",
    "actions": [
      {
        "trigger": "ON_CLICK",
        "workflow": "LAUNCH_QUESTIONNAIRE",
        "id": "householdRegister",
        "questionnaire": {
          "id": "f210a832-857f-49e6-93f5-399eec4f4edb",
          "title": "{{add.family}}",
          "saveButtonText": "Add Household",
          "setPractitionerDetails": true,
          "setOrganizationDetails": true
        }
      }
    ]
  },
  "staticMenu": [
    {
      "id": "reports",
      "visible": true,
      "display": "{{reports}}",
      "menuIconConfig": {
        "type": "local",
        "reference": "ic_reports"
      },
      "actions": [
        {
          "trigger": "ON_CLICK",
          "workflow": "LAUNCH_REPORT",
          "id": "defaultMeasureReport"
        }
      ]
    },
    {
      "id": "p2p_sync",
      "visible": true,
      "display": "{{transfer.data}}",
      "menuIconConfig": {
        "type": "local",
        "reference": "ic_p2p"
      },
      "actions": [
        {
          "trigger": "ON_CLICK",
          "workflow": "DEVICE_TO_DEVICE_SYNC",
          "id": "navigateToP2PScreen"
        }
      ]
    },
    {
      "id": "profile",
      "visible": true,
      "display": "{{profile }}",
      "menuIconConfig": {
        "type": "local",
        "reference": "ic_settings"
      },
      "actions": [
        {
          "trigger": "ON_CLICK",
          "workflow": "LAUNCH_SETTINGS",
          "id": "navigateToSettingsScreen"
        }
      ]
    }
  ],
  "clientRegisters": [
    {
      "id": "householdRegister",
      "display": "{{ all.households }}",
      "menuIconConfig": {
        "type": "remote",
        "reference": "Binary/144177"
      },
      "showCount": true,
      "actions": [
        {
          "trigger": "ON_CLICK",
          "workflow": "LAUNCH_REGISTER",
          "id": "householdRegister"
        },
        {
          "trigger": "ON_COUNT",
          "id": "householdRegister"
        }
      ]
    },
    {
      "id": "ancRegister",
      "display": "ANC",
      "menuIconConfig": {
        "type": "remote",
        "reference": "Binary/145193"
      },
      "showCount": true,
      "actions": [
        {
          "trigger": "ON_CLICK",
          "workflow": "LAUNCH_REGISTER",
          "id": "ancRegister"
        },
        {
          "trigger": "ON_COUNT",
          "id": "ancRegister"
        }
      ]
    },
    {
      "id": "taskRegister",
      "display": "Tasks",
      "menuIconConfig": {
        "type": "local",
        "reference": "ic_tasks_priority"
      },
      "showCount": true,
      "actions": [
        {
          "trigger": "ON_CLICK",
          "workflow": "LAUNCH_REGISTER",
          "id": "taskRegister"
        },
        {
          "trigger": "ON_COUNT",
          "id": "taskRegister"
        }
      ]
    }
  ]
}
