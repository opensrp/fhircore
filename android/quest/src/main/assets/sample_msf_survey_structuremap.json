            },
            {
              "context": "condition",
              "contextType": "variable",
              "element": "clinicalStatus",
              "transform": "cc",
              "parameter": [
                {
                  "valueString": "http://terminology.hl7.org/CodeSystem/condition-clinical"
                },
                {
                  "valueString": "active"
                }
              ]
            },
            {
              "context": "condition",
              "contextType": "variable",
              "element": "verificationStatus",
