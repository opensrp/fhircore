          "trigger": "ON_CLICK",
          "workflow": "LAUNCH_QUESTIONNAIRE",
          "questionnaire": {
            "id": "58fbddae-c5a0-4b86-832e-f516c96f3b85",
            "title": "Record sick child",
            "resourceIdentifier": "@{patientId}",
